{
  "name": "workingon-studio",
  "type": "module",
  "version": "1.3.0",
  "scripts": {
    "dev": "npm run timeline:generate && astro dev",
    "build": "npm run timeline:generate && astro build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "astro preview",
    "astro": "astro",
    "timeline:generate": "node scripts/generateTimeline.js",
    "timeline:fetch-github": "node scripts/fetchGitHubTimeline.js",
    "timeline:status": "node -e \"const fs=require('fs'); const t=JSON.parse(fs.readFileSync('src/data/timeline.json')); console.log('Source:', t.source||'local-git', '| Generated:', new Date(t.generated).toLocaleString());\""
  },
  "dependencies": {
    "@astrojs/svelte": "^7.1.0",
    "@astrolib/seo": "^1.0.0-beta.8",
    "@felte/validator-zod": "^1.0.18",
    "@iconify/svelte": "^5.0.1",
    "@octokit/rest": "^22.0.0",
    "@tailwindcss/vite": "^4.1.13",
    "astro": "^5.14.4",
    "dotenv": "^17.2.2",
    "felte": "^1.3.0",
    "luxon": "^3.7.2",
    "svelte": "^5.38.7",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/luxon": "^3.7.1",
    "@types/node": "^20.19.11",
    "@types/umami": "^2.10.1",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14"
  }
}
