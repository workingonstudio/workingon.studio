{
  "name": "workingon-studio",
  "type": "module",
  "version": "1.3.0",
  "scripts": {
    "dev": "npm run timeline:generate && astro dev",
    "build": "npm run timeline:generate && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "timeline:generate": "node scripts/generateTimeline.js",
    "timeline:fetch-github": "node scripts/fetchGitHubTimeline.js",
    "timeline:status": "node -e \"const fs=require('fs'); const t=JSON.parse(fs.readFileSync('src/data/timeline.json')); console.log('Source:', t.source||'local-git', '| Generated:', new Date(t.generated).toLocaleString());\""
  },
  "dependencies": {
    "@astrojs/svelte": "^7.1.0",
    "@octokit/rest": "^21.1.1",
    "@tailwindcss/vite": "^4.1.12",
    "astro": "^5.13.4",
    "dotenv": "^17.2.2",
    "luxon": "^3.7.2",
    "svelte": "^5.38.5",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/luxon": "^3.7.1",
    "@types/node": "^20.19.11"
  }
}
